  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rm -rf dist && cross-env NODE_ENV=production webpack  --mode production --config build/webpack/webpack.prod.conf.js",
    "serve": "npx serve -l 8181 -s dist",
    "dev": "cross-env NODE_ENV=development webpack-dev-server  --mode development --config build/webpack/webpack.dev.conf.js",
    "test:clean-vr-test": "rm -rf vr-report",
    "test:vr-test": "npm run test:clean-vr-test && node tests/vr/test.js"
  },
